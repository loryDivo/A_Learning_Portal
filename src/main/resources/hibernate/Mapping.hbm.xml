<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
	<class name="com.assignment3.models.User" table="users">
		<id name="id" type="int" column="id">
		</id>
		
		<property name="username" column="username" type="string" length="45" not-null="true" />
		
		<property name="password" column="password" type="string" length="45" not-null="true" />
		
		<property name="name" column="name" type="string" length="45" not-null="true" />
		
		<property name="last_name" column="last_name" type="string" length="45" not-null="true" />
		
		<property name="email" column="email" type="string" length="45" not-null="true" />

	</class>
</hibernate-mapping>